@model IEnumerable<Car>;     

<div class="container pt-5">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Model</th>
                <th scope="col">Make</th>
                <th scope="col">Year</th>
                <th scope="col">
                </th>
            </tr>
        </thead>
       <tbody>
           @foreach(Car item in Model)
            {
           <tr>
               <th scope="row">@item.Id</th>
               <td>@item.Model</td>
               <td>@item.Make</td>
               <td>@item.Year</td>
               <td>
                   <a href="/car/info/@item.Id">View Detailed Info</a>
               </td>
           </tr>

            }
       </tbody>
    </table>

</div>